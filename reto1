import csv
import requests
import json
#lista de IP's
filename = open('D:\Prueba tecnica\Prueba de capacidades t√©cnicas - Listado de participantes - reto No. 1.csv', 'r')

file = csv.DictReader(filename)
ipaddress = ()

for col in file:
    ipaddress.append (col['GDPR IP'])

print('GDPR IP', ipaddress)

#lista de paises con IP
x = 0
for x in ipaddress:
    
    request_url = 'https://geolocation-db.com/jsonp/'
    response = requests.get(request_url)
    result = response.content.decode()
    result = result.split("(")[1].strip(")")
    result  = json.loads(result)

print(result['country_name'])

